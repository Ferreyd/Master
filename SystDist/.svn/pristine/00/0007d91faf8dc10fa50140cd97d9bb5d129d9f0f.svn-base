package fr.unice.miage.m1.distributedsystems.tp4.rmi_basics;

import java.rmi.Remote;

public interface Server extends Remote {

    void method1(int value) throws java.rmi.RemoteException;

    void method2(MyObject object) throws java.rmi.RemoteException;

    int method3() throws java.rmi.RemoteException;

    MyObject method4() throws java.rmi.RemoteException;

    void method5(Server server) throws java.rmi.RemoteException;

    void test() throws java.rmi.RemoteException;

    Server returnServer() throws java.rmi.RemoteException;

}
